extends base

block files
    link(rel="stylesheet", href="css/reusablePage.css")
    script(defer src="js/resuablePage.js") 
    link(rel="stylesheet", href="css/individualProject.css")
    script(defer  src="js/individualProject.js")  
    link(rel="stylesheet", href="css/mediaQueries.css")

block content 
    <div class="div-carousel div-carousel-overlay hidden-carousel">
        <div class="carousel-overlay-blur"></div>
        <!-- Close button -->
        <ion-icon class="btn-close-carousel" name="close-outline"></ion-icon>
        <!-- Left arrow -->
        <ion-icon class="scroll-arrow scroll-left" name="chevron-back-outline"></ion-icon>

        <div class="overlay-content"></div>

        <!-- Right arrow -->
        <ion-icon class="scroll-arrow scroll-right" name="chevron-forward-outline"></ion-icon>
    </div>
    .page.page-project 
        .mainp-header
            img.p-main-img(src='https://images.pexels.com/photos/17077794/pexels-photo-17077794/free-photo-of-tilt-house-behind-grass-on-montmartre.jpeg', alt='')
            .div-project-header
                .p-header-txt
                    h1.p-header-title Project Title
                    p.p-header-para Delta needed a wonderful space planning for their office.
                .p-header-overlay
                    .p-overlay-align
                        h1.overlay-title Client 
                        p.overlay-subtitle.p-client-name Name
                    .p-overlay-align
                        h1.overlay-title Location 
                        p.overlay-subtitle.p-location San Francisco, CA
                    .p-overlay-align
                        h1.overlay-title Duration 
                        p.overlay-subtitle.p-duration 6 Months
                    .p-overlay-align
                        h1.overlay-title Service 
                        p.overlay-subtitle.p-service Space Planning

